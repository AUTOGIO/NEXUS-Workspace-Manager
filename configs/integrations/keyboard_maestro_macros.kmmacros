          "ActionUID" : "YABAI-QUICK-WINDOW-SCRIPT",
          "Script" : "#!/bin/zsh\n\n# YABAI Quick Window Management\n# Keyboard Maestro Integration\n\n# Get current window info\nCURRENT_WINDOW=$(yabai -m query --windows --window 2>/dev/null)\n\nif [ $? -eq 0 ]; then\n    # Extract window info\n    WINDOW_ID=$(echo $CURRENT_WINDOW | jq -r '.id')\n    APP_NAME=$(echo $CURRENT_WINDOW | jq -r '.app')\n    DISPLAY=$(echo $CURRENT_WINDOW | jq -r '.display')\n    SPACE=$(echo $CURRENT_WINDOW | jq -r '.space')\n    \n    echo \"Current Window: $APP_NAME (ID: $WINDOW_ID)\"\n    echo \"Location: Display $DISPLAY, Space $SPACE\"\n    echo \"\"\n    echo \"Quick Actions:\"\n    echo \"1. Move to next display\"\n    echo \"2. Toggle float\"\n    echo \"3. Rotate layout\"\n    echo \"4. Focus ChatGPT\"\n    echo \"5. Focus Cursor\"\n    \n    # Example: Move to next display\n    NEXT_DISPLAY=$((DISPLAY % 3 + 1))\n    yabai -m window --display $NEXT_DISPLAY\n    echo \"✅ Moved $APP_NAME to Display $NEXT_DISPLAY\"\nelse\n    echo \"❌ No focused window found\"\nfi",
          "Shell" : "/bin/zsh",
          "UseText" : false
        }
